-- Scripts:
function LUAZIFIER_SCRIPT1() -- Luazifier.TextButton1
local script = Instance.new("LocalScript")
script.Parent = Luazifier.TextButton1
script.Disabled = false


script.Parent.MouseButton1Click:Connect(function()
    local Target = Luazifier.TextBox1.Text
        print ("Is in workspace")
	print (Target)
local tool
for i,v in pairs (game.Players:GetPlayers()) do
	tool = v.Backpack:FindFirstChild'OddPotion' or v.Character:FindFirstChild'OddPotion'
	if tool then break end
end

for i,v in pairs (game.Players:GetPlayers()) do
	tool = v.Backpack:FindFirstChild'OddPotion' or v.Character:FindFirstChild'OddPotion'
	if tool then break end
end

for i,v in pairs(workspace:WaitForChild(Target).Head:GetChildren()) do
	tool.TransEvent:FireServer(v,1)
end

for i,v in pairs(game.Players:WaitForChild(Target).Character:GetChildren()) do
	if v.Name == "Head" and v:GetDescendants() then
		tool.TransEvent:FireServer(v,1)
		end
	end
end)




end
coroutine.resume(coroutine.create(LUAZIFIER_SCRIPT1))

function LUAZIFIER_SCRIPT2() -- Luazifier.Frame1
local script = Instance.new("LocalScript")
script.Parent = Luazifier.Frame1
script.Disabled = false

-- Enjoy! :D


function dragify(Frame)
	dragToggle = nil
	dragSpeed = .25 -- You can edit this.
	dragInput = nil
	dragStart = nil
	dragPos = nil

	function updateInput(input)
		Delta = input.Position - dragStart
		Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
		game:GetService("TweenService"):Create(Frame, TweenInfo.new(.25), {Position = Position}):Play()
	end

	Frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
			dragToggle = true
			dragStart = input.Position
			startPos = Frame.Position
			input.Changed:Connect(function()
				if (input.UserInputState == Enum.UserInputState.End) then
					dragToggle = false
				end
			end)
		end
	end)

	Frame.InputChanged:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if (input == dragInput and dragToggle) then
			updateInput(input)
		end
	end)
end
dragify(script.Parent) 

end
coroutine.resume(coroutine.create(LUAZIFIER_SCRIPT2))

function LUAZIFIER_SCRIPT3() -- Luazifier.TextButton2
local script = Instance.new("LocalScript")
script.Parent = Luazifier.TextButton2
script.Disabled = false

script.Parent.MouseButton1Click:Connect(function()
	script.Parent.Parent.Visible = false  -- parent parent = Frame 
	print ("closing UI")
end)

ragdoll:Remove()

end
coroutine.resume(coroutine.create(LUAZIFIER_SCRIPT3))

